{
  "name": "@micra/graphql-service-template",
  "version": "0.0.1",
  "author": "Olavo Amorim Santos<contact@micra.dev>",
  "license": "MIT",
  "main": ".micra/index.cjs.js",
  "module": ".micra/index.js",
  "types": ".micra/index.d.ts",
  "files": [
    ".micra/**/*"
  ],
  "scripts": {
    "build": "yarn rimraf ./.micra && yarn merge-schemas && cross-env APP_ENV=dev NODE_ENV=development tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "build:staging": "yarn rimraf ./.micra && yarn merge-schemas && cross-env APP_ENV=staging NODE_ENV=production tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "build:prod": "yarn rimraf ./.micra && yarn merge-schemas && cross-env APP_ENV=prod NODE_ENV=production tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "format": "prettier --write 'src/**/*.{ts,tsx}' --config .prettierrc.js",
    "lint": "eslint 'src/**/*.{ts,tsx}' --cache",
    "merge-schemas": "node .config/build/merge-schemas",
    "start": "nodemon --config .config/dev/nodemon.json",
    "test": "jest --config .config/test/jest.config.ts"
  },
  "volta": {
    "node": "14.15.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@jest/types": "^26.6.2",
    "@micra/cli": "^0.0.25",
    "@types/body-parser": "^1.19.0",
    "@types/cls-hooked": "^4.3.3",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.20",
    "@types/mock-express-request": "^0.2.0",
    "@types/node": "^14.14.25",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "apollo-graphql": "^0.6.0",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^3.2.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "fast-glob": "^3.2.5",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-docblock": "^26.0.0",
    "jest-runner": "^26.6.3",
    "jest-runner-groups": "^2.0.1",
    "mock-express-request": "^0.2.2",
    "mock-express-response": "^0.3.0",
    "node-factory": "^0.4.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "tsc-alias": "^1.2.6",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@apollo/federation": "^0.21.0",
    "@micra/apollo-express-kernel": "^0.0.5",
    "@micra/application": "^0.0.11",
    "@micra/core": "^0.0.8",
    "@micra/dot-env": "^0.0.3",
    "@micra/kernel": "^0.0.3",
    "@micra/micra-graphql-container": "^0.0.1",
    "@micra/service-provider": "^0.0.2",
    "@micra/typedi-service-container": "^0.0.1",
    "@micra/validation-rules": "^0.0.9",
    "@micra/validator": "^0.0.7",
    "apollo-server-core": "^2.19.2",
    "apollo-server-express": "^2.19.2",
    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "reflect-metadata": "^0.1.13",
    "ts-deepmerge": "^1.0.6",
    "tslib": "^2.1.0",
    "typedi": "^0.10.0",
    "uuid": "^8.3.2"
  }
}
